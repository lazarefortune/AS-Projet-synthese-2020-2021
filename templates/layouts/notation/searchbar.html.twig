<div class="headeer">
    <h3 class="text-center title">Liste des projets</h3>

    <div class="input-group search">
        <input id="input_search" type="search" class="form-control" placeholder="Titre de projet" aria-label="groupSearch" aria-describedby="basic-addon2" onKeyUp="findItem()">
        <button type="button" class="btn btn-primary" onclick="findItem();">
            <i class="fas fa-search"></i>
